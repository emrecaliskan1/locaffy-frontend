{
  "expo": {
    "name": "Locaffy",
    "slug": "locaffy-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/locaffyicon2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1C1E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zenvero.locaffymobileapp",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Bu uygulama profil fotoğrafı seçmek için fotoğraf galerinize erişim istemektedir.",
        "NSCalendarsUsageDescription": "Bu uygulama rezervasyon hatırlatmaları oluşturmak için takviminize erişim istemektedir."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/locaffyicon2.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "backgroundColor": "#1C1C1E",
        "resizeMode": "native"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zenvero.locaffymobileapp",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "apiBaseUrl": "https://api.locaffy.store/api",
      "eas": {
        "projectId": "401baaa9-939d-4d08-a160-caddc3ee9a84"
      }
    }
  }
}